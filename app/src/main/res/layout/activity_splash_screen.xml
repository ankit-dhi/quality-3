<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.ankitsapplication.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSplashScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelight_blue_A200">
        <ImageView android:id="@+id/imageLogo"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_72pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_151pxh"
            android:layout_marginStart="@dimen/_151pxh"
            android:layout_marginTop="@dimen/_326pxv"
            android:layout_width="@dimen/_72pxh"
            android:scaleType="fitXY" imageRes="@{@drawable/img_logo}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_logo"/>
    </FrameLayout>
</layout>
